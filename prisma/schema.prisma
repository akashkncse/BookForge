generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id        String   @id @default(cuid())
  title     String
  author    String
  desc      String
  image     String
  available Boolean  @default(true)
  createdAt DateTime @default(now())
}
